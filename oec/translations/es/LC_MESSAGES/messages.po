#, python-format
msgid "The total world trade in %(year)s was %(export_val)s."
msgstr "El comercio mundial total en %(year)s fue de %(export_val)s."

#, python-format
msgid "The 10 most traded products by dollar amount are %(exports_list)s, using the 1992 revision of the HS (Harmonized System) classification."
msgstr "Los 10 productos más comercializados por cantidad en dólares son %(exports_list)s, utilizando la revisión de la clasificación del SA (Sistema Armonizado) 1992."

#, python-format
msgid "The top 10 exporting countries are %(origins_list)s."
msgstr "Los 10 principales países exportadores son %(origins_list)s."

#, python-format
msgid "%(country_is)s the %(econ_rank)s largest export economy in the world"
msgstr "%(country_is)s la %(econ_rank)s mayor economía de exportación en el mundo"

#, python-format
msgid "and the %(eci_rank)s most complex economy according to the Economic Complexity Index (ECI)."
msgstr "y la economía más compleja %(eci_rank)s de acuerdo con el Índice de Complejidad Económica (ECI)."

#, python-format
msgid "In %(year)s, %(country)s exported $%(export_val)s and imported $%(import_val)s, resulting in a %(positive_negative)s trade balance of $%(trade_delta)s."
msgstr "En %(year)s, %(country)s exportó $ %(export_val)s e importó $ %(import_val)s, dando como resultado un saldo comercial %(positive_negative)s de $ %(trade_delta)s."

#, python-format
msgid "In %(year)s the GDP %(of_country)s was $%(gdp)s and its GDP per capita was $%(gdp_pc)s."
msgstr "En %(year)s el PIB %(of_country)s fue de $ %(gdp)s y su PIB per cápita fue de $ %(gdp_pc)s."

#, python-format
msgid "The top exports %(of_country)s are %(exports_list)s, using the 1992 revision of the HS (Harmonized System) classification. Its top imports are %(imports_list)s."
msgstr "Las principales exportaciones de %(of_country)s son %(exports_list)s, de acuerdo ala clasificación del Sistema Harmonizado (HS). Sus principales importaciones son %(imports_list)s."

#, python-format
msgid "The top export destinations %(of_country)s are %(destinations)s. The top import origins are %(origins)s."
msgstr "Los principales destinos de las exportaciones de %(of_country)s son %(destinations)s. Los principales orígenes de sus importaciones son %(origins)s."

#, python-format
msgid "%(country)s is an island and borders %(maritime_borders)s by sea."
msgstr "%(country)s es una isla que limita con %(maritime_borders)s por mar."

#, python-format
msgid "%(country)s borders %(land_borders)s."
msgstr "%(country)s limita con %(land_borders)s."

#, python-format
msgid "%(country)s borders %(land_borders)s by land and %(maritime_borders)s by sea."
msgstr "%(country)s limita con %(land_borders)s por tierra y con %(maritime_borders)s por mar."

#, python-format
msgid "During the last five years exports have %(increased_decreased)s at a median annualized rate of %(change_rate)s%%, from $%(past_export_val)s in %(past_year)s to $%(current_export_val)s in %(current_year)s."
msgstr "Durante los últimos cinco años las exportaciones han %(increased_decreased)s a una tasa anual promedio de %(change_rate)s%%, pasando de $ $%(past_export_val)s en %(past_year)s a $ %(current_export_val)s en %(current_year)s."

#, python-format
msgid "The most recent exports are led by %(top_export)s which represent %(top_export_pct)s%% of the total products exported, followed by %(second_export)s, which account for %(second_export_pct)s%%."
msgstr "Las exportaciones más recientes son guiados %(top_export)s que representan el %(top_export_pct)s%% del total de productos exportados, seguido de %(second_export)s, que representan el %(second_export_pct)s%%."

#, python-format
msgid "The top exporters globally are %(origins)s."
msgstr "Los principales exportadores a nivel mundial son %(origins)s."

msgid "Origins"
msgstr "Orígenes"

#, python-format
msgid "In %(year)s %(country)s exported $%(export_val)s, making it the %(export_rank)s largest exporter in the world."
msgstr "En %(year)s %(country)s exportó $%(export_val)s, lo que es el %(export_rank)s exportador más grande en el mundo."

#, python-format
msgid "During the last five years the exports %(of_country)s have %(increased_decreased)s at an annualized rate of %(change_rate)s%%, from $%(past_export_val)s in %(past_year)s to $%(current_export_val)s in %(current_year)s."
msgstr "Durante los últimos cinco años las exportaciones %(of_country)s han %(increased_decreased)s a una tasa anualizada del %(change_rate)s%%, de $%(past_export_val)s en %(past_year)s a $%(current_export_val)s en %(current_year)s."

#, python-format
msgid "The most recent exports are led by %(top_export)s which represent %(top_export_pct)s%% of the total exports %(of_country)s, followed by %(second_export)s, which account for %(second_export_pct)s%%."
msgstr "Las exportaciones más recientes son lideradas por la exportación de %(top_export)s, que representa el %(top_export_pct)s%%de las exportaciones totales %(of_country)s, seguidas por %(second_export)s, que representan el %(second_export_pct)s%%."

#, python-format
msgid "In %(year)s %(country)s imported $%(import_val)s, making it the %(import_rank)s largest importer in the world."
msgstr "En %(year)s %(country)s importó $%(import_val)s, lo que es el %(import_rank)s importador más grande en el mundo."

#, python-format
msgid "During the last five years the imports %(of_country)s have %(increased_decreased)s at an annualized rate of %(change_rate)s%%, from $%(past_import_val)s in %(past_year)s to $%(current_import_val)s in %(current_year)s."
msgstr "Durante los últimos cinco años las importaciones %(of_country)s han %(increased_decreased)s a una tasa anualizada del %(change_rate)s%%, de $%(past_import_val)s en %(past_year)s a $%(current_import_val)s en %(current_year)s."

#, python-format
msgid "The most recent imports are led by %(top_import)s which represent %(top_import_pct)s%% of the total imports %(of_country)s, followed by %(second_import)s, which account for %(second_import_pct)s%%."
msgstr "Las importaciones más recientes son lideradas por %(top_import)s, que representa el %(top_import_pct)s%% de las importaciones totales %(of_country)s, seguido por %(second_import)s, que representa el %(second_import_pct)s%%."

#, python-format
msgid "The top export destinations %(of_country)s are %(destinations)s."
msgstr "Los principales destinos de las exportaciones %(of_country)s son %(destinations)s."

#, python-format
